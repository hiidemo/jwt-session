{
    "name": "hiidemo/jwt-session",
    "description": "JwtSession is a PHP session replacement. Instead of use FileSystem, just use JWT TOKEN. The implementation following the SessionHandlerInterface.",
    "authors": [
        {
            "name": "João Gilberto Magalhães",
            "email": "joao@byjg.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "ByJG\\Session\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
		"type": "vcs",
		"url": "https://github.com/hiidemo/jwt-wrapper"
	}
    ],
    "require": {
        "php": ">=5.6.0",
        "hiidemo/jwt-wrapper": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|7.4.*"
    },
    "license": "MIT"
}
